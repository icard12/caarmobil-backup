{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "electron:dev": "concurrently \"vite\" \"npm run server\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "build:full": "npx prisma generate && npm run build && npm run server:build",
    "electron:build": "npm run build && npm run server:build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "server": "tsx --watch server/index.ts",
    "server:build": "esbuild server/index.ts --bundle --platform=node --target=node20 --outfile=dist-server/index.cjs --external:prisma --external:@prisma/client --external:fsevents",
    "start": "npx prisma db push && node dist-server/index.cjs"
  },
  "build": {
    "appId": "com.callmobile.caarmobil",
    "productName": "CAAR MOBIL",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "dist-server/**/*",
      "electron/**/*",
      "server/**/*",
      "prisma/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "src/assets/logo.png"
    },
    "forceCodeSigning": false
  },
  "dependencies": {
    "@prisma/client": "5.19.0",
    "@supabase/supabase-js": "^2.57.4",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "framer-motion": "^12.29.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.344.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pg": "^8.17.2",
    "prisma": "5.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "wait-on": "^9.0.3"
  }
}